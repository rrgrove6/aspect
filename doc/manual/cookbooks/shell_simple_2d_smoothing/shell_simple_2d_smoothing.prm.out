# An extension of the 2d shell simple cookbook that is used to show the
# benefits of using the artificial viscosity smoothing feature. See the
# manual for more information.

set %%\hyperref[parameters:Dimension]{Dimension}%                              = 2%% \index[prmindex]{Dimension} \index[prmindexfull]{Dimension} %
set %%\hyperref[parameters:Use years in output instead of seconds]{Use years in output instead of seconds}% = false%% \index[prmindex]{Use years in output instead of seconds} \index[prmindexfull]{Use years in output instead of seconds} %
set %%\hyperref[parameters:End time]{End time}%                               = 1.5e16%% \index[prmindex]{End time} \index[prmindexfull]{End time} %
set %%\hyperref[parameters:Output directory]{Output directory}%                       = output%% \index[prmindex]{Output directory} \index[prmindexfull]{Output directory} %

subsection %%\hyperref[parameters:Discretization]{Discretization}%
  set %%\hyperref[parameters:Discretization/Temperature polynomial degree]{Temperature polynomial degree}% = 2%% \index[prmindex]{Temperature polynomial degree} \index[prmindexfull]{Discretization!Temperature polynomial degree} %

  subsection %%\hyperref[parameters:Discretization/Stabilization_20parameters]{Stabilization parameters}%
    set %%\hyperref[parameters:Discretization/Stabilization parameters/Use artificial viscosity smoothing]{Use artificial viscosity smoothing}% = true%% \index[prmindex]{Use artificial viscosity smoothing} \index[prmindexfull]{Discretization!Stabilization parameters!Use artificial viscosity smoothing} %
  end
end

subsection %%\hyperref[parameters:Material_20model]{Material model}% 
  set %%\hyperref[parameters:Material model/Model name]{Model name}% = simple%% \index[prmindex]{Model name} \index[prmindexfull]{Material model!Model name} %

  subsection %%\hyperref[parameters:Material_20model/Simple_20model]{Simple model}%
    set %%\hyperref[parameters:Material model/Simple model/Thermal expansion coefficient]{Thermal expansion coefficient}% = 4e-5  %% \index[prmindex]{Thermal expansion coefficient} \index[prmindexfull]{Material model!Simple model!Thermal expansion coefficient} %
    set %%\hyperref[parameters:Material model/Simple model/Viscosity]{Viscosity}%                     = 1e22  %% \index[prmindex]{Viscosity} \index[prmindexfull]{Material model!Simple model!Viscosity} %
  end
end


subsection %%\hyperref[parameters:Geometry_20model]{Geometry model}%
  set %%\hyperref[parameters:Geometry model/Model name]{Model name}% = spherical shell%% \index[prmindex]{Model name} \index[prmindexfull]{Geometry model!Model name} %

  subsection %%\hyperref[parameters:Geometry_20model/Spherical_20shell]{Spherical shell}%
    set %%\hyperref[parameters:Geometry model/Spherical shell/Inner radius]{Inner radius}%  = 3481000%% \index[prmindex]{Inner radius} \index[prmindexfull]{Geometry model!Spherical shell!Inner radius} %
    set %%\hyperref[parameters:Geometry model/Spherical shell/Outer radius]{Outer radius}%  = 6336000%% \index[prmindex]{Outer radius} \index[prmindexfull]{Geometry model!Spherical shell!Outer radius} %
    set %%\hyperref[parameters:Geometry model/Spherical shell/Opening angle]{Opening angle}% = 90%% \index[prmindex]{Opening angle} \index[prmindexfull]{Geometry model!Spherical shell!Opening angle} %
  end
end


subsection %%\hyperref[parameters:Model_20settings]{Model settings}%
  set %%\hyperref[parameters:Model settings/Zero velocity boundary indicators]{Zero velocity boundary indicators}%       = inner%% \index[prmindex]{Zero velocity boundary indicators} \index[prmindexfull]{Model settings!Zero velocity boundary indicators} %
  set %%\hyperref[parameters:Model settings/Tangential velocity boundary indicators]{Tangential velocity boundary indicators}% = outer, left, right%% \index[prmindex]{Tangential velocity boundary indicators} \index[prmindexfull]{Model settings!Tangential velocity boundary indicators} %
  set %%\hyperref[parameters:Model settings/Prescribed velocity boundary indicators]{Prescribed velocity boundary indicators}% =%% \index[prmindex]{Prescribed velocity boundary indicators} \index[prmindexfull]{Model settings!Prescribed velocity boundary indicators} %

  set %%\hyperref[parameters:Model settings/Fixed temperature boundary indicators]{Fixed temperature boundary indicators}%   = inner, outer%% \index[prmindex]{Fixed temperature boundary indicators} \index[prmindexfull]{Model settings!Fixed temperature boundary indicators} %

  set %%\hyperref[parameters:Model settings/Include shear heating]{Include shear heating}%                   = true%% \index[prmindex]{Include shear heating} \index[prmindexfull]{Model settings!Include shear heating} %
end


subsection %%\hyperref[parameters:Boundary_20temperature_20model]{Boundary temperature model}%
  set %%\hyperref[parameters:Boundary temperature model/Model name]{Model name}% = spherical constant%% \index[prmindex]{Model name} \index[prmindexfull]{Boundary temperature model!Model name} %
  subsection %%\hyperref[parameters:Boundary_20temperature_20model/Spherical_20constant]{Spherical constant}%
    set %%\hyperref[parameters:Boundary temperature model/Spherical constant/Inner temperature]{Inner temperature}% = 4273%% \index[prmindex]{Inner temperature} \index[prmindexfull]{Boundary temperature model!Spherical constant!Inner temperature} %
    set %%\hyperref[parameters:Boundary temperature model/Spherical constant/Outer temperature]{Outer temperature}% = 973%% \index[prmindex]{Outer temperature} \index[prmindexfull]{Boundary temperature model!Spherical constant!Outer temperature} %
  end
end


subsection %%\hyperref[parameters:Initial_20conditions]{Initial conditions}%
  set %%\hyperref[parameters:Initial conditions/Model name]{Model name}% = spherical hexagonal perturbation%% \index[prmindex]{Model name} \index[prmindexfull]{Initial conditions!Model name} %
end


subsection %%\hyperref[parameters:Gravity_20model]{Gravity model}%
  set %%\hyperref[parameters:Gravity model/Model name]{Model name}% = radial earth-like%% \index[prmindex]{Model name} \index[prmindexfull]{Gravity model!Model name} %
end


subsection %%\hyperref[parameters:Mesh_20refinement]{Mesh refinement}%
  set %%\hyperref[parameters:Mesh refinement/Initial global refinement]{Initial global refinement}%          = 6%% \index[prmindex]{Initial global refinement} \index[prmindexfull]{Mesh refinement!Initial global refinement} %
  set %%\hyperref[parameters:Mesh refinement/Initial adaptive refinement]{Initial adaptive refinement}%        = 0%% \index[prmindex]{Initial adaptive refinement} \index[prmindexfull]{Mesh refinement!Initial adaptive refinement} %
  set %%\hyperref[parameters:Mesh refinement/Strategy]{Strategy}%                           = temperature%% \index[prmindex]{Strategy} \index[prmindexfull]{Mesh refinement!Strategy} %
end


subsection %%\hyperref[parameters:Postprocess]{Postprocess}%
  set %%\hyperref[parameters:Postprocess/List of postprocessors]{List of postprocessors}% = visualization, velocity statistics, temperature statistics, heat flux statistics, depth average%% \index[prmindex]{List of postprocessors} \index[prmindexfull]{Postprocess!List of postprocessors} %

  subsection %%\hyperref[parameters:Postprocess/Visualization]{Visualization}%
    set %%\hyperref[parameters:Postprocess/Visualization/Output format]{Output format}%                 = vtu%% \index[prmindex]{Output format} \index[prmindexfull]{Postprocess!Visualization!Output format} %
    set %%\hyperref[parameters:Postprocess/Visualization/Time between graphical output]{Time between graphical output}% = 1e6%% \index[prmindex]{Time between graphical output} \index[prmindexfull]{Postprocess!Visualization!Time between graphical output} %
    set %%\hyperref[parameters:Postprocess/Visualization/Number of grouped files]{Number of grouped files}%       = 0%% \index[prmindex]{Number of grouped files} \index[prmindexfull]{Postprocess!Visualization!Number of grouped files} %
  end

  subsection %%\hyperref[parameters:Postprocess/Depth_20average]{Depth average}%
    set %%\hyperref[parameters:Postprocess/Depth average/Time between graphical output]{Time between graphical output}% = 115%% \index[prmindex]{Time between graphical output} \index[prmindexfull]{Postprocess!Depth average!Time between graphical output} %
  end
end

subsection %%\hyperref[parameters:Compositional_20fields]{Compositional fields}%
  set %%\hyperref[parameters:Compositional fields/Number of fields]{Number of fields}% = 1%% \index[prmindex]{Number of fields} \index[prmindexfull]{Compositional fields!Number of fields} %
end

subsection %%\hyperref[parameters:Compositional_20initial_20conditions]{Compositional initial conditions}%
  set %%\hyperref[parameters:Compositional initial conditions/Model name]{Model name}% = function%% \index[prmindex]{Model name} \index[prmindexfull]{Compositional initial conditions!Model name} %

  subsection %%\hyperref[parameters:Compositional_20initial_20conditions/Function]{Function}%
    set %%\hyperref[parameters:Compositional initial conditions/Function/Variable names]{Variable names}%      = x,y%% \index[prmindex]{Variable names} \index[prmindexfull]{Compositional initial conditions!Function!Variable names} %
    set %%\hyperref[parameters:Compositional initial conditions/Function/Function expression]{Function expression}% = if(sqrt(x*x+y*y)<4000000,1,0)%% \index[prmindex]{Function expression} \index[prmindexfull]{Compositional initial conditions!Function!Function expression} %
  end
end